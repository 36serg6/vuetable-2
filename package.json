{
  "name": "vuetable-2",
  "version": "1.6.5",
  "description": "Datatable component for Vue 2.x",
  "main": "dist/vuetable-2.js",
  "license": "MIT",
  "author": "Rati Wannapanop <rati.wannapanop@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ratiw/vuetable-2.git"
  },
  "private": false,
  "keywords": [
    "vue",
    "component",
    "table",
    "datatable",
    "pagination-components",
    "json"
  ],
  "browserify": {
    "transform": [
      "babelify",
      [
        "vueify",
        {
          "_flags": {
            "debug": true
          }
        }
      ]
    ]
  },
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "node build/build.js",
    "bundle:min": "cross-env MINIFY=true webpack --progress --config build/webpack.build.config.js",
    "bundle:full": "cross-env MINIFY=false webpack --progress --config build/webpack.build.config.js",
    "bundle": "npm run bundle:min && npm run bundle:full",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.15.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-jest": "21.2.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.3.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "inject-loader": "^3.0.0",
    "isparta-loader": "^2.0.0",
    "jest": "21.2.1",
    "jest-serializer-vue": "0.2.0",
    "jest-vue": "0.8.1",
    "jest-vue-preprocessor": "1.2.0",
    "json-loader": "^0.5.4",
    "lolex": "^1.5.2",
    "opn": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "ora": "^1.2.0",
    "phantomjs-prebuilt": "^2.1.3",
    "promise-polyfill": "^6.0.2",
    "rimraf": "^2.6.0",
    "selenium-server": "^3.0.1",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "stats-webpack-plugin": "0.4.3",
    "url-loader": "^0.5.8",
    "vue": "^2.5.2",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "1.2.3",
    "vue-loader": "^12.1.0",
    "vue-server-renderer": "^2.5.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "vue-test-utils": "1.0.0-beta",
    "vueify": "^9.4.1",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "@(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "mapCoverage": true,
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
